<div class="row">
{{# each products }}
    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img class="img-responsive" src="{{this.imagePath}}" alt=""></img>
              <div class="caption">
                  
        <h3>{{this.title}}</h3>
        <p>	{{this.description}}</p>

<div class="clearfix">
    <div class="price">${{this.price}}</div>
        <a href="" class="btn btn-primary pull-right">購買</a>
    
</div>

              </div>
      
        </div>

    </div>
{{/each}}
    
</div>